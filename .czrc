{
  "$schema": "https://cdn.jsdelivr.net/gh/Zhengqbbb/cz-git@1.9.2/docs/public/schema/cz-git.json",
  "path": "node_modules/cz-git",
  "alias": {
    ":": "docs: update posts",
    "f": "docs: fix typos",
    "r": "docs: update README",
    "a": "feat(theme): update page theme",
    "b": "build: bump dependencies",
    "s": "style: update code format",
    "i": "feat: initial commit",
    "c": "chore: update config"
  },
  "scopes": ["posts", "theme", "script", "config"],
  "skipQuestions": ["body", "breaking", "footerPrefix", "footer"],
  "customScopesAlign": "top"
}
