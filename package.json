{
  "private": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "dev": "vite --host --port 3333",
    "build": "pnpm build:prev && vite-ssg build && pnpm build:post",
    "build:prev": "run-p -c gen:sh  gen:og",
    "build:post": "run-p -c gen:rss gen:sitemap",
    "serve": "vite preview --host",
    "cz": "czg",
    "lint": "eslint .",
    "z:image": "tsx ./build/node/zImage.ts",
    "gen:sh": " tsx ./build/node/syntaxHightlight.ts",
    "gen:og": " tsx ./build/node/og/main.mts",
    "gen:rss": "tsx ./build/node/rss.ts",
    "gen:sitemap": "tsx ./build/node/sitemap.ts"
  },
