    "fast-glob": "^3.3.1",
    "feed": "^4.2.2",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "imagemin": "^8.0.1",
    "imagemin-optipng": "^8.0.0",
