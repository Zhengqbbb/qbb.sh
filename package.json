{
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "run-p dev:main dev:docs",
    "dev:docs": "astro dev --host --port 3000",
    "dev:main": "esbuild src/main.ts --watch --bundle --format=esm --outdir=src/lib/client",
    "build": "run-s gen:og build:main build:docs",
    "build:docs": "astro check && astro build",
    "build:main": "esbuild src/main.ts --minify --bundle --format=esm --outdir=src/lib/client",
    "gen:og": "tsx src/lib/server/og/main.ts",
    "preview": "astro preview --host --port 8888",
    "lint": "eslint .",
    "cz": "czg"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@astrojs/check": "^0.7.0",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@iconify/json": "^2.2.222",
    "@resvg/resvg-js": "^2.6.2",
    "@types/node": "20.11.1",
    "@types/react": "^18.3.3",
    "@unocss/astro": "^0.61.0",
    "@unocss/eslint-plugin": "^0.61.0",
    "@unocss/reset": "^0.61.0",
    "@vite-pwa/astro": "^0.4.0",
    "astro": "^4.11.1",
    "astro-eslint-parser": "^1.0.2",
    "czg": "^1.9.3",
    "eslint": "^9.5.0",
    "eslint-plugin-astro": "^1.2.2",
    "eslint-plugin-format": "^0.1.2",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "mdast-util-from-markdown": "^2.0.1",
    "medium-zoom": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "pathe": "^1.1.2",
    "prettier-plugin-astro": "^0.14.0",
    "react": "^18.3.1",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "remark-directive": "^3.0.0",
    "remark-github-alerts": "0.1.0-beta.3",
    "satori": "^0.10.13",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2",
    "unocss": "^0.61.0",
    "vite-plugin-restart": "^0.4.1",
    "workbox-window": "^7.1.0"
  }
}
