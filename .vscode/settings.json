{
  "cSpell.words": [
    "'s",
    "antfu",
    "demi",
    "iconify",
    "intlify",
    "pinia",
    "pnpm",
    "Qbenben",
    "unocss",
    "unplugin",
    "Vite",
    "vitejs",
    "Vitesse",
    "vitest",
    "vueuse"
  ],
  "cSpell.files": [
    "src"
  ],
  "i18n-ally.sourceLanguage": "zh-CN",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": "locales",
  "i18n-ally.sortKeys": true,
  // use eslint
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": false,
  "files.associations": {
    "*.css": "css",
    ".czrc": "json",
    "_redirects": "properties",
    "**/.x-cmd/*": "shellscript",
    "**/.x-cmd/*.yml": "yaml",
  },
  "explicitFolding.rules": {
    "css": [
        {
            "beginRegex": "^\\/\\*\\*\\sSection:?",
            "endRegex": "^\\/\\*\\*\\sEndSection",
            "autoFold": true
        }
    ],
    "shellscript": [
        {
            "beginRegex": "^\\s*#\\s*Section:?",
            "endRegex": "^\\s*#\\s*EndSection",
            "autoFold": true
        }
    ]
},
  "picgo.picBed.github.path": "blog/",
  "cssvar.files": [
    "src/styles/vars.css"
  ],
  "css.lint.unknownAtRules": "ignore"
}
